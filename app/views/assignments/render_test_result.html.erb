<div id="test_results" class="tabbedArea">
  <% test_script_results = TestScriptResult.find_all_by_grouping_id(@grouping.id) %>

  <ul>
  <% test_script_results.each do |ts_result| %>
  	<% test_script = TestScript.find(ts_result.test_script_id) %>
  	<li>
  	  Script name: <%= test_script.script_name %>
  	  <ul>
  	  <li>Marks earned: <%= ts_result.marks_earned %></li>
   	  <% test_results = TestResult.find_all_by_grouping_id_and_test_script_id(ts_result.grouping_id, ts_result.test_script_id) %>
  	  <% test_results.each do |t_result| %>
 	    <li>Test name: <%= t_result.name %></li>
   	    <ul>
          <li>Description: <%= t_result.description %></li>
          <li>Marks_earned: <%= t_result.marks_earned %></li>
          <li>Marks available: <%= t_result.marks_available %></li>
          <li>Feedback: <%= t_result.feedback %></li>
        </ul>
  	  <% end %>
  	  </ul>
  	</li>
  <% end %>
  </ul>
</div> <!-- Test Results -->
